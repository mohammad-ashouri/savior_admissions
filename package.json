{
    "private": true,
    "type": "module",
    "scripts": {
        "dev": "vite",
        "build-app": "vite build",
        "build": "npm-run-all build-app copy-jquery copy-select2 copy-export-images copy-chartjs copy-datatable-select copy-datatable-SearchPanes copy-font-files copy-datatable copy-datatable-buttons copy-datatable-jszip copy-datatable-pdfmake copy-datatable-colreorder",
        "copy-select2": "mkdirp public/build/plugins && ncp node_modules/select2 public/build/plugins/select2",
        "copy-datatable": "mkdirp public/build/plugins && ncp resources/OtherLibraries/DataTables public/build/plugins/DataTables",
        "copy-datatable-buttons": "mkdirp public/build/plugins && ncp resources/OtherLibraries/Buttons-3.1.2 public/build/plugins/Buttons-3.1.2",
        "copy-datatable-jszip": "mkdirp public/build/plugins && ncp resources/OtherLibraries/jszip public/build/plugins/jszip",
        "copy-datatable-pdfmake": "mkdirp public/build/plugins && ncp node_modules/pdfmake public/build/plugins/pdfmake",
        "copy-datatable-colreorder": "mkdirp public/build/plugins && ncp resources/OtherLibraries/ColReorder-2.0.4 public/build/plugins/ColReorder-2.0.4",
        "copy-datatable-select": "mkdirp public/build/plugins && ncp resources/OtherLibraries/Select-2.1.0 public/build/plugins/Select-2.1.0",
        "copy-datatable-SearchPanes": "mkdirp public/build/plugins && ncp resources/OtherLibraries/SearchPanes public/build/plugins/SearchPanes",
        "copy-jquery": "mkdirp public/build/plugins && ncp node_modules/jquery public/build/plugins/jquery",
        "copy-export-images": "mkdirp public/build/export-images && ncp resources/images/export-pdf-images public/build/export-images",
        "copy-font-files": "mkdirp public/build/Fonts && ncp resources/css/Fonts public/build/Fonts",
        "copy-chartjs": "mkdirp public/build/plugins && ncp resources/OtherLibraries/ChartJs-4.4.0 public/build/plugins/ChartJs-4.4.0"
    },
    "devDependencies": {
        "autoprefixer": "^10.4.15",
        "axios": "^1.1.2",
        "flowbite": "^1.8.1",
        "laravel-vite-plugin": "^0.8.1",
        "postcss": "^8.4.29",
        "sass": "^1.80.4",
        "select2": "^4.1.0-rc.0",
        "tailwindcss": "^3.3.3",
        "vite": "^4.0.0"
    },
    "dependencies": {
        "@fortawesome/fontawesome-free": "^6.5.1",
        "@vitejs/plugin-vue": "^4.2.3",
        "alpinejs": "^3.14.1",
        "animate.css": "^4.1.1",
        "apexcharts": "^4.0.0",
        "font-awesome": "^4.7.0",
        "ionicons": "^7.2.2",
        "jquery": "^3.7.1",
        "jquery-ui-dist": "^1.13.2",
        "lodash": "^4.17.21",
        "mkdirp": "^3.0.1",
        "moment": "^2.30.1",
        "ncp": "^2.0.0",
        "npm-run-all": "^4.1.5",
        "pdfmake": "^0.2.14",
        "photoswipe": "^5.4.4",
        "sweetalert2": "^11.6.13"
    }
}
